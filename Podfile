platform :ios, '9.0'

target 'Capstone' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Capstone
  pod 'GoogleSignIn'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'SwiftyJSON'
  pod 'Alamofire'

end
